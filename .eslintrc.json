{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "airbnb/hooks"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "import/no-named-as-default": 0,
        "import/prefer-default-export": "off",
        "react/jsx-filename-extension": "off",
        "react/prefer-stateless-function": "off"
    }
}